{
  "name": "shelfie",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "migration:create": "dotenv -- sequelize-cli migration:create",
    "migrate": "dotenv -- sequelize-cli db:migrate",
    "migration:rollback": "dotenv -- sequelize-cli db:migrate:undo",
    "migration:rollback:all": "dotenv -- sequelize-cli db:migrate:undo:all",
    "migration:seed": "dotenv -- sequelize-cli db:seed:all",
    "migration:seed:undo": "dotenv -- sequelize-cli db:seed:undo:all",
    "migration:reseed": "dotenv -- sequelize-cli db:drop && dotenv -- sequelize-cli db:create && dotenv -- sequelize-cli db:migrate && dotenv -- sequelize-cli db:seed:all"
  },
  "dependencies": {
    "@apollo/client": "^3.13.8",
